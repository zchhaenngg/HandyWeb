@using HandyWork.Common.EntityFramework.Query
@using HandyWork.Model
@model AuthUser
@using (Html.BeginForm("JsonFindUsers", "Account", FormMethod.Post, new { @class = "form-inline", role = "form" }))
{
    <div class="form-group">
        @Html.LabelFor(m => m.RealName, new { @class = "sr-only" })
        @Html.TextBoxFor(m => m.RealName, new { name = "[Like]" + nameof(AuthUser.RealName), @class = "form-control", placeholder = "请输入姓名" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.UserName, new { @class = "sr-only" })
        @Html.TextBoxFor(m => m.UserName, new { name = "[Like]" + nameof(AuthUser.UserName), @class = "form-control", placeholder = "请输入用户名" })
    </div>
    <button type="submit" class="btn btn-default">提交</button>
}
<script>
    $("#@nameof(AuthUser.RealName)").attr("name", "[Like]@nameof(AuthUser.RealName)")
    $("#@nameof(AuthUser.UserName)").attr("name", "[Like]@nameof(AuthUser.UserName)")
</script>